{
    "name": "danhunsaker/laravel-braintree",
    "description": "A Braintree bridge for Laravel",
    "keywords": ["laravel", "framework", "Laravel-Braintree", "Laravel Braintree"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    },
    {
        "name": "Dan Hunsaker",
        "email": "danhunsaker@gmail.com"
    }],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^5.1|^6.0|^7.0|^8.0",
        "graham-campbell/manager": "^3.0|^4.0",
        "braintree/braintree_php": "^3.27|^4.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0|^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Danhunsaker\\Braintree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danhunsaker\\Tests\\Braintree\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Danhunsaker\\Braintree\\BraintreeServiceProvider"
            ],
            "aliases": {
                "Braintree": "Danhunsaker\\Braintree\\Facades\\Braintree"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
